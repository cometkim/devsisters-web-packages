{
  "name": "<{ projectNameKebab }>",
  "private": true,
  "scripts": {
    "gatsby": "sucrase-node ./node_modules/gatsby/dist/bin/gatsby.js",
    "start": "npm run gatsby -- serve",
    "clean": "rm -rf ./public",
    "lint": "prettier --check \"src/**/*.@(ts|tsx|scss)\"",
    "lint:fix": "prettier --write \"src/**/*.@(ts|tsx|scss)\"",
    "build:stage": "DOTENV=stage npm run gatsby -- build --prefix-paths",
    "build:production": "DOTENV=production npm run gatsby -- build --prefix-paths",
    "dev": "run-p dev:**",
    "dev:gatsby": "DOTENV=dev npm run gatsby -- develop -H 0.0.0.0",
    "dev:prettier": "onchange '@(*.ts|@(src|types)/**/*.@(ts|tsx|scss))' -- prettier --write {{changed}}"
  },
  "lint-staged": {
    "*.{ts|tsx}": "prettier --check"
  },
  "husky": {
    "hooks": {
      "pre-push": "lint-staged"
    }
  },
  "dependencies": {
    "@devsisters/gatsby-hats": "^1.6.0",
    "@devsisters/gatsby-stack": "^1.1.1",
    "@types/node": "^10.14.7",
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "gatsby": "^2.8.0",
    "gatsby-link": "^2.1.1",
    "husky": "^2.3.0",
    "lint-staged": "^8.2.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^5.2.0",
    "prettier": "^1.17.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "sucrase": "^3.10.1",
    "typescript": "^3.4.5"
  }
}
